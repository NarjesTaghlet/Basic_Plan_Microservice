  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:ngrok": "node start-ngrok.js"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.812.0",
    "@aws-sdk/client-codebuild": "^3.799.0",
    "@aws-sdk/client-codepipeline": "^3.797.0",
    "@aws-sdk/client-codestar-connections": "^3.797.0",
    "@aws-sdk/client-dynamodb": "^3.810.0",
    "@aws-sdk/client-iam": "^3.782.0",
    "@aws-sdk/client-lightsail": "^3.840.0",
    "@aws-sdk/client-rds": "^3.841.0",
    "@aws-sdk/client-s3": "^3.797.0",
    "@aws-sdk/client-secrets-manager": "^3.787.0",
    "@aws-sdk/client-sts": "^3.782.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/aws-sdk": "^0.0.42",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.10.0",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "simple-git": "^3.27.0",
    "typeorm": "^0.3.22",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "ngrok": "^4.3.3",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
